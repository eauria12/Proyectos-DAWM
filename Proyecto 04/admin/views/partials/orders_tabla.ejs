<div class="table-responsive">
  <div class="table-wrapper">
    <div class="table-title">
      <div class="row">
        <div class="col-sm-5">
          <h2><b>Orders</b></h2>
        </div>
        <table class="table table-striped table-hover">
          <thead>
            <tr>
              <th>Order Code</th>
              <th>Order Date</th>
              <th>Product Line</th>
              <th>Product Name</th>
              <th>Quantity Ordered</th>
              <th>Price Each</th>
              <th>Required Date</th>
              <th>Shipped Date</th>
              <th>Status</th>
            </tr>
          </thead>
          <tbody>
            <% arrOrders.forEach((order,idx) => { %>
              <tr>
                <td><%= order.orderNumber %></td>
                <td><%= order.orderDate %></td>
                <td><%= order.productLine %></td>
                <td><%= order.productName %></td>
                <td><%= order.quantityOrdered %></td>
                <td>$<%= order.priceEach %></td>
                <td><%= order.requiredDate %></td>
                <td><%= order.shippedDate %></td>
                <td><div class="sta"><%= order.status %></div></td>        
              </tr>
              <% }); %>
          </tbody>
        </table>
        <div class="total">
          <h4><b>Total:</b> $1000000000 </h4>
        </div> 
      </div>
    </div>